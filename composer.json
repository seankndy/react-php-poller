{
  "name": "seankndy/react-php-poller",
  "description": "monitoring poller using ReactPHP",
  "type": "project",
  "require": {
    "php": ">=7.4",
    "ext-eio": "*",
    "ext-event": "*",
    "evenement/evenement": "^3.0",
    "psr/log": "^1.1",
    "react/child-process": "^0.6.1",
    "react/promise": "^2.7",
    "react/mysql": "^0.5.2",
    "react/dns": "^0.4.16",
    "react/http-client": "^0.5.9",
    "react/socket": "^1.2",
    "react/filesystem": "^0.1.2",
    "ramsey/uuid": "^3.8",
    "react/datagram": "^1.5",
    "ext-json": "*",
    "ext-bcmath": "*"
  },
  "autoload": {
    "psr-4": {
      "SeanKndy\\Poller\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SeanKndy\\Poller\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Sean Kennedy",
      "email": "sean@kndy.net"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "react/async": "^3.0"
  }
}
