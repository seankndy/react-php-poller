{
  "name": "seankndy/react-php-poller",
  "description": "monitoring poller using ReactPHP",
  "type": "project",
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "ext-bcmath": "*",
    "evenement/evenement": "^3.0",
    "psr/log": "^1.1",
    "react/child-process": "^0.6.5",
    "react/promise": "^2.7",
    "react/mysql": "^0.5.2",
    "react/dns": "^1.10.0",
    "react/http-client": "^0.5.11",
    "react/socket": "^1.12.0",
    "react/filesystem": "^0.1.2",
    "ramsey/uuid": "^3.8",
    "react/datagram": "^1.5",
    "nesbot/carbon": "^2.62"
  },
  "autoload": {
    "psr-4": {
      "SeanKndy\\Poller\\": "src/"
    },
    "files": ["src/functions.php"]
  },
  "autoload-dev": {
    "psr-4": {
      "SeanKndy\\Poller\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Sean Kennedy",
      "email": "sean@kndy.net"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "react/async": "^3.0",
    "spatie/test-time": "^1.3",
    "vimeo/psalm": "^4.27"
  },
  "config": {
    "allow-plugins": {
      "wyrihaximus/composer-update-bin-autoload-path": true
    }
  }
}
